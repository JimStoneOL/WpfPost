<Page x:Class="WpfPost.Pages.RegisterPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfPost.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegisterPage">

    <Grid>
        <Label Content="Register Page" HorizontalAlignment="Left" Margin="285,10,0,0" VerticalAlignment="Top" Height="42" Width="273"/>
        <TextBox x:Name="LoginText" HorizontalAlignment="Left" Height="45" Margin="147,153,0,0" TextWrapping="Wrap" Text="Введите email" VerticalAlignment="Top" Width="411"/>
        <TextBox x:Name="PasswordText" HorizontalAlignment="Left" Height="45" Margin="147,225,0,0" TextWrapping="Wrap" Text="Введите пароль" VerticalAlignment="Top" Width="411"/>
        <Button Content="Зарегистрироваться" HorizontalAlignment="Left" Height="51" Margin="147,331,0,0" VerticalAlignment="Top" Width="196" Click="Button_Click"/>
        <Button Content="Войти" HorizontalAlignment="Left" Height="51" Margin="362,331,0,0" VerticalAlignment="Top" Width="196" Click="Button_Click"/>
        <TextBox x:Name="FioText" HorizontalAlignment="Left" Height="45" Margin="147,81,0,0" TextWrapping="Wrap" Text="Введите фио" VerticalAlignment="Top" Width="411"/>
    </Grid>
</Page>
